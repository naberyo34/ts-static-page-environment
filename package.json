    "@types/stylelint": "^9.10.1",
    "@types/webpack-stream": "^3.2.11",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@webpack-cli/init": "^1.1.3",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.12",
    "core-js": "^3.10.0",
