    "@types/eslint": "^7.2.10",
    "@types/gulp": "^4.0.8",
    "@types/gulp-autoprefixer": "^0.0.32",
    "@types/gulp-imagemin": "^7.0.2",
    "@types/gulp-plumber": "^0.0.32",
    "@types/htmlhint": "^0.9.2",
    "@types/imagemin-pngquant": "^8.0.0",
    "@types/jest": "^26.0.23",
    "@types/prettier": "^2.2.3",
    "@types/stylelint": "^9.10.1",
    "@types/webpack-stream": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@webpack-cli/init": "^1.1.3",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.12",
    "core-js": "^3.12.1",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-ejs": "^5.1.0",
    "gulp-eslint": "^6.0.0",
    "gulp-htmlhint": "^4.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-prettier": "^3.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-stylelint": "^13.0.0",
    "htmlhint": "^0.14.1",
    "husky": "4",
    "imagemin-pngquant": "^9.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpack-stream": "^6.1.2"
  },
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "eslint": "eslint '_dev/js/**/*.{js,ts}'",
    "eslint:fix": "eslint --fix '_dev/js/**/*.{js,ts}'",
    "stylelint": "stylelint '_dev/scss/**/*.scss'",
    "stylelint:fix": "stylelint --fix '_dev/scss/**/*.scss'",
    "format": "prettier --write --loglevel=warn '_dev/**/*.{pug,js,ts,scss}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "_dev/pug/**/*.pug": [
      "prettier --write"
    ],
    "_dev/js/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "_dev/scss/**/*.scss": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "tsConfig": "tsconfig.json"
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
