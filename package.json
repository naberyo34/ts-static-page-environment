    "gulp-pug": "^4.0.1",
    "gulp-stylelint": "^13.0.0",
    "htmlhint": "^0.14.1",
    "husky": "4",
    "imagemin-pngquant": "^9.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-stream": "^6.1.2"
  },
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "eslint": "eslint '_dev/js/**/*.{js,ts}'",
    "eslint:fix": "eslint --fix '_dev/js/**/*.{js,ts}'",
    "stylelint": "stylelint '_dev/scss/**/*.scss'",
    "stylelint:fix": "stylelint --fix '_dev/scss/**/*.scss'",
    "format": "prettier --write --loglevel=warn '_dev/**/*.{js,ts,scss}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "_dev/js/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "_dev/scss/**/*.scss": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "tsConfig": "tsconfig.json"
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
